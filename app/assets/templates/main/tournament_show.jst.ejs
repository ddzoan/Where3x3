<div class="header-image" style="background-image: url(assets/<%= Math.floor(7*Math.random()) %>.jpg)">
</div>

<div class="container" style="margin-top: 20px">
  <div class="row">
    <div class="col-lg-8">
      <div class="row">
        <div class="col-md-3">
          <div class="round">
            <a href="javascript:void(0)">
              <img src="http://lorempixel.com/150/150/sports/"/>
            </a>
          </div>
          Organizer: The Organizer
        </div>
        <div class="col-md-9">
          <div class="name">
            <%= tournament.escape('name') %>
          </div>
          <div class="venue">
            <%= tournament.escape('venue') %>
          </div>
        </div>
      </div>
    </div>
    <div class="col-lg-4">
      <form>
        <div class="form-group">
          <label>Name
            <input type="text" class="form-control" name="name" placeholder="name">
          </label>
        </div>
        <div class="checkbox">
          Events:<br>
          <label><input type="checkbox" name="333">3x3x3</label><br>
          <label><input type="checkbox" name="444">4x4x4</label><br>
          <label><input type="checkbox" name="555">5x5x5</label><br>
          <label><input type="checkbox" name="222">2x2x2</label><br>
          <label><input type="checkbox" name="bld">3x3x3 Blindfolded</label><br>
          <label><input type="checkbox" name="oh">3x3x3 One Handed</label><br>
          <label><input type="checkbox" name="fm">3x3x3 Fewest Moves</label><br>
          <label><input type="checkbox" name="feet">3x3x3 with Feet</label><br>
          <label><input type="checkbox" name="minx">Megaminx</label><br>
          <label><input type="checkbox" name="pyraminx">Pyraminx</label><br>
          <label><input type="checkbox" name="sq1">Square 1</label><br>
          <label><input type="checkbox" name="clock">Rubik's Clock</label><br>
          <label><input type="checkbox" name="skewb">Skewb</label><br>
          <label><input type="checkbox" name="666">6x6x6</label><br>
          <label><input type="checkbox" name="777">7x7x7</label><br>
          <label><input type="checkbox" name="bld4">4x4x4 Blindfolded</label><br>
          <label><input type="checkbox" name="bld5">5x5x5 Blindfolded</label><br>
          <label><input type="checkbox" name="multibld">Multi-Blind</label><br>
        </div>
        <button type="submit" class="btn btn-default">Register now!</button>
      </form>
    </div>
  </div>
  <div class="row">
    <div class="dates">
      <%= new Date(tournament.escape('start_date')).toDateString() %>
      <% if(tournament.escape('start_date') < tournament.escape('end_date')){ %>
        - <%= new Date(tournament.escape('end_date')).toDateString() %>
      <% } %>
    </div>
  </div>
  <div class="row">
    <div class="location">
      <%= tournament.escape('location') %>
    </div>
  </div>
  <div class="row">
    <div class="static-map">
    </div>
  </div>
</div>
