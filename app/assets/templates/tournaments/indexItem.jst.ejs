<div class="info">
  <div class="tournament-image">
    <img src="/assets/<%= Math.floor(7*Math.random()) %>.jpg"/>
    <div class="date-overlay">
      <%= tournament.get('start_date') %>
      <% if(tournament.get('end_date') !== tournament.get('start_date')){ %>
        - <%= tournament.get('end_date') %>
      <% } %>
    </div>
  </div>
  <div class="tournament-details">
    <div class="name">
      <%= tournament.escape('name') %>
    </div>
    <div class="venue">
      <%= tournament.escape('venue') %>
    </div>
  </div>
</div>
